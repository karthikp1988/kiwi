        <script><![CDATA[function runCode(){
	
  
 var startdatetime = g_form.getValue('start_date_time');
 var student = g_form.getDisplayValue('student');
 var tutor = g_form.getDisplayValue('tutor');
 var glax = new GlideAjax('CheckforConflict');
 glax.addParam('sysparm_name', 'checkforeventconflict');
 glax.addParam('sysparm_start_time',startdatetime);
 glax.addParam('sysparm_student',student);
 glax.addParam('sysparm_tutor',tutor);
 glax.getXML(CheckConflictParse);
 
function CheckConflictParse(response) {
	
	var answer = response.responseXML.documentElement.getAttribute("answer");
    //alert(answer);
	if(answer =="You Cannot Schedule"){ g_form.setValue('conflict_status','cannot_schedule');g_form.save();}
	else if(answer =="You Can Schedule"){g_form.setValue('conflict_status','schedule');g_form.save();}
 
}
}]]></script>
